{
  "name": "@qwickapps/react-framework",
  "version": "1.5.6",
  "type": "module",
  "description": "Complete React framework with responsive navigation, flexible layouts, theming system, and reusable components for building modern applications.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "create": "./scripts/create-qwickapps-project.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "require": "./dist/config.js",
      "import": "./dist/config.esm.js"
    },
    "./dist/index.css": "./dist/index.css",
    "./index.css": "./dist/index.css"
  },
  "sideEffects": [
    "**/*.css",
    "./src/schemas/transformers/registry.ts",
    "./src/schemas/index.ts",
    "./dist/schemas/transformers/registry.js",
    "./dist/schemas/index.js"
  ],
  "files": [
    "dist",
    "src",
    "scripts",
    "QUICK_START_GUIDE.md"
  ],
  "scripts": {
    "build": "rollup -c && npm run build-palettes",
    "build-palettes": "node scripts/build-palettes.cjs",
    "build:dev": "NODE_ENV=development rollup -c && npm run build-palettes",
    "build:prod": "NODE_ENV=production STRIP_LOGS=true rollup -c && npm run build-palettes",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist node_modules .rollup.cache storybook-static",
    "create-project": "node scripts/create-qwickapps-project.js",
    "validate:clean-install": "./qa/clean-install/validate.sh",
    "prepublishOnly": "npm run build && npm run validate:clean-install",
    "publish": "npm publish",
    "publish:palettes": "../../products/qwickapps/anvil/publish-palettes.sh",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "framework",
    "responsive",
    "navigation",
    "layouts",
    "theme",
    "palette",
    "dark-mode",
    "components",
    "typescript",
    "ui-kit",
    "qwickapps"
  ],
  "author": "QwickApps",
  "license": "PolyForm-Shield-1.0.0",
  "dependencies": {
    "@qwickapps/schema": "^1.3.2",
    "marked": "^4.3.0",
    "md5": "^2.3.0",
    "sanitize-html": "^2.17.0"
  },
  "optionalDependencies": {
    "@qwickapps/logging": "^1.0.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.0.0",
    "@emotion/styled": ">=11.0.0",
    "@mui/icons-material": ">=5.0.0",
    "@mui/material": ">=5.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.18.0",
    "@babel/preset-typescript": "^7.27.1",
    "@chromatic-com/storybook": "^4.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-docs": "^9.1.0",
    "@storybook/react-vite": "^9.1.0",
    "@storybook/react-webpack5": "^9.1.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^30.0.0",
    "@types/marked": "^5.0.2",
    "@types/md5": "^2.3.5",
    "@types/mustache": "^4.2.5",
    "@types/node": "^24.1.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.16.0",
    "babel-jest": "^29.0.0",
    "css-loader": "^7.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "react-router-dom": "^6.30.1",
    "rollup": "^3.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "globals": {
      "import.meta": {
        "env": {
          "DEV": false,
          "PROD": true,
          "MODE": "test"
        }
      }
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(.*\\.mjs$))"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.performance\\.test\\.(ts|tsx)$",
      "\\.serialization\\.test\\.(ts|tsx)$",
      "HeroBlock\\.test\\.(ts|tsx)$",
      "Footer\\.test\\.(ts|tsx)$",
      "Logo\\.test\\.(ts|tsx|js)$",
      "FeatureGrid\\.integration\\.test\\.(ts|tsx)$",
      "FeatureCard\\.integration\\.test\\.(ts|tsx)$"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qwickapps/react-framework.git"
  },
  "homepage": "https://github.com/qwickapps/react-framework#readme"
}
